        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    // Manage Out of Stock by Transfer Order Phase Transit
    if(previous.install_status == '6' && current.install_status == '9'){
		(new StockMovementUtils()).createOutTOMovementStock(previous.stockroom, current.quantity,current);
    };

	// Manage Entry in Stock by Tranfer Order Phase Received 
	if (previous.install_status == '9' && current.install_status == '6') {
        (new StockMovementUtils()).createINTOMovementStock(current.stockroom, current.quantity, current);
    };

})(current, previous);]]></script>
