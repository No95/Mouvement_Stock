        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Manage the partial receipt Transfer Order for Consumables.
	// Analysis between Quantity Requested and Quantity Received (Si la Qte réceptionnée n'est pas égale à la quantité commandée alors réception partielle)
	var requested = parseInt(current.quantity_requested, 10);
	var received = parseInt(current.quantity_received, 10);
	var previousreceived = parseInt(previous.quantity_received, 10);
	var remaining = parseInt(current.quantity_remaining, 10);

	if(previousreceived == 0 && requested == received){
		gs.info('Réception globale de consommable');
	}
	else {
		// On vérifie qu'i y a bien eu au moins une réception
		if(requested > remaining) {
			(new StockMovementUtils()).createPartialRecConsumableMovementStock(current.transfer_order.to_stockroom, current, (current.quantity_received - previous.quantity_received ));
		}
	}
	

})(current, previous);]]></script>
